<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicbox">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Arabic_Songs"
            android:label="@string/Arabic_Songs"
            android:configChanges="orientation|screenSize"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".English_Songs"
            android:label="@string/English_Songs"
            android:configChanges="orientation|screenSize"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Songs_Langugh"
            android:label="@string/Songs_Langugh"
            android:parentActivityName=".MainActivity"
            />
    </application>

</manifest>